{
    "manifest_version": 2,

    "name": "Getting started example",
    "description": "This extension shows a Google Image search result for the current page",
    "version": "1.0",
    "background": {
        "scripts": ["./JavaScript/eventPage.js"],
        "persistent": false
    },


    "browser_action": {
        "default_icon": "./Source/favi16.png",
        "default_title": "Open a new tab!"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "https://ajax.googleapis.com/",
        "http://104.223.65.66:5000/*",
        "http://127.0.0.1:5000/*"
    ],

    "icons": {
        "16": "./Source/favi16.png",
        "48": "./Source/favi48.png",
        "128": "./Source/favi128.png"
    }
}
